    <center>
        <VBox spacing="10" alignment="CENTER">
            <ListView fx:id="playlistView"/>
            <Button text="Create Playlist" onAction="#handleCreatePlaylist"/>
            <Button text="Delete Playlist" onAction="#handleDeletePlaylist"/>
        </VBox>
    </center>
</BorderPane>